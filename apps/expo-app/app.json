{
  "expo": {
    "name": "Portable Games",
    "slug": "Chrispio-Gaming",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chrispio.portagames"
    },
    "android": {
      "package": "com.chrispio.portagames",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Digital-Regular.ttf",
            "./assets/fonts/Digital-Bold.ttf",
            "./assets/fonts/Digital-Italic.ttf",
            "./assets/fonts/Digital-ItalicBold.ttf"
          ]
        }
      ]
    ]
  }
}
